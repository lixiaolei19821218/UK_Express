    </membership>
    <httpRuntime targetFramework="4.5" />
    <authentication mode="Forms">
      <forms loginUrl="~/accounts/login" defaultUrl="/Default.aspx" />
    </authentication>
  </system.web>
  <location path="products">
    <system.web>
      <authorization>
        <allow roles="users" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
  <location path="cart">
    <system.web>
      <authorization>
        <allow roles="users" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
  <location path="accounts/UserCentre">
